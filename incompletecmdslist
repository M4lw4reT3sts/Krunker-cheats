function ran84714()
	local s = {
		{"a","b","c","d","f","g","h","l","x","m"},
		{1,2,3,4,5,6,7,8,9,0}
	}
	local a = ""
	local function r(...) return math.random(...) end
	for i = 1,10 do
		local b = r(1,2)
		if b == 1 then
			local c = s[r(1,2)][r(1,10)]
			if type(c) == "string" then
				c = c:upper()
			end
			a = a..c
		else
			a = a..s[r(1,2)][r(1,10)]
		end
	end
	return(tostring(a))
end
