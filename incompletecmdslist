			D9J28v2:Destroy()
		end
		DJ2a2.Touched:Connect(function() end)
	end
	cDJd82ne.Parent = workspace.Terrain._Game.Workspace
	DhD22q6Dh:Destroy()
	notif("you are immune to obby kill parts")
end

